{
  "faction_info": {
    "name": "Tyranids",
    "description": "The Great Devourer, an alien swarm controlled by a collective Hive Mind.",
    "faction_rules": [
      {
        "name": "Objectives",
        "effect": "Tyranid armies cannot score Victory Points from holding Objective counters. Instead: 1 VP for each detachment destroyed. 1 VP for each quadrant of the playing field not occupied by enemy forces each turn."
      },
      {
        "name": "The Hive Mind",
        "effect": "One Synapse Creature must be taken for every 3 Core Broods chosen. Bio-Titans and Flyers do not count toward this limit."
      },
      {
        "name": "Brood",
        "effect": "The term 'Brood' is used instead of 'Detachment' but has the exact same meaning."
      }
    ]
  },
  "unique_orders": {
    "rampage": {
      "name": "Rampage",
      "description": "Move at double rate towards nearest enemy. Must fight if in contact. Moves around impassable terrain via shortest path."
    },
    "hunt": {
      "name": "Hunt",
      "description": "Move normal rate towards nearest enemy not in combat. Will engage if possible. If not pinned, shoots at nearest enemy in Advance Fire segment."
    },
    "nest": {
      "name": "Nest",
      "description": "Will not move. Shoots at nearest enemy in First Fire segment."
    }
  },
  "psychic_powers": {
    "psychic_scream": {
      "name": "Psychic Scream",
      "effect": "First Fire phase. Morale check (caster). If passed, all units within 8\" must take Morale test; if failed, they Fall Back."
    },
    "catalyst": {
      "name": "Catalyst",
      "effect": "Combat phase. Morale check (caster). If passed, a single brood within 8\" gains Feel No Pain."
    },
    "the_horror": {
      "name": "The Horror",
      "effect": "Before rolling engagement die. Morale check (caster). If passed, enemy rolls one extra die but discards the highest (CAF applies as normal)."
    }
  },
  "formations": {
    "synaptic_swarm": {
      "name": "Synaptic Swarm",
      "flavor": "The archetypical swarm linked to the Hive Mind.",
      "compulsory": ["HQ", "Core", "Core", "Core"],
      "optional": ["HQ", "Core", "Core", "Support", "Support", "Support", "Vanguard", "Vanguard", "Artillery", "Transport", "Air Support", "Titan", "Knight"],
      "rules": ["One Synapse Creature must be taken for every 3 Core Broods."]
    },
    "assault_swarm": {
      "name": "Assault Swarm",
      "flavor": "Waves of Mycetic Spores launched from orbit.",
      "compulsory": ["HQ", "Core", "Core", "Core"],
      "optional": ["HQ", "Core", "Core", "Support", "Support", "Support", "Vanguard", "Vanguard", "Artillery", "Transport", "Air Support", "Titan", "Knight"],
      "rules": [
        "Mycetic Spore Assault: All broods must be carried in Mycetic Spores unless they have Deep Strike, Infiltrate, or Flyer rules.",
        "Player may deploy Deep Strike broods from turn one."
      ]
    },
    "bio_titan_swarm": {
      "name": "Bio-Titan Swarm",
      "flavor": "Late stage invasion with massive bio-constructs.",
      "compulsory": ["Titan", "Titan"],
      "optional": ["HQ", "Core", "Core", "Support", "Support", "Support", "Vanguard", "Vanguard", "Artillery", "Transport", "Air Support", "Titan", "Knight"],
      "rules": ["Late Stage Invasion: All units in this formation re-roll failed Morale checks."]
    }
  },
  "special_rules_library": {
    "synapse": "Aura effect (X inches). Units within range auto-pass Morale checks (except Psychic tests).",
    "instinct": "Take Morale check at end of Initiative Phase. If failed, unit receives the specified order (Rampage, Hunt, Nest).",
    "regeneration": "When reduced to 0 wounds, do not remove. In End Phase, if not wiped out, roll D6 per wound lost; 6+ restores 1 wound.",
    "chameleon_skin": "Cannot be targeted by enemy units engaged outside of X inches.",
    "winged": "Can partake in close combat even if they have Flyer rule.",
    "burrow": "Instead of moving, return to reserves to Deep Strike in later turns.",
    "spawn": "Spawn Y units of type X. Added to brood if same type, else form new brood. Placed close to spawner.",
    "miasma": "Shooting attacks against this unit suffer -1 To Hit. Enemies engaged with this unit suffer -1 CAF."
  },
  "weapon_traits_library": {
    "attract": "If target not killed, move closest model D6\" towards attacker. Can cause Engagement. Ignored by Void/Ion Shields.",
    "hunger": "When enemy slain, attacker regains 1 wound (up to max).",
    "beam": "Trace straight line to target. All formations under line are hit.",
    "parry": "In Close Combat, force opponent to re-roll one die.",
    "spore_launcher": "Target closest model in detachment. Roll 2D6 + scatter. If hit, detonates as Spore Mine.",
    "spore_mine": "Moves randomly D6\" in End Phase. Detonates on contact with non-Tyranid or impassable. Blast (3\"), AP -1, Light AT."
  },
